  <xs:complexType name="KmrbRating">
    <xs:complexContent mixed="false">
      <xs:extension base="fev2:Rating">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="RatingDescriptors" type="sa:ArrayOfstring"/>
          <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="RatingName" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
