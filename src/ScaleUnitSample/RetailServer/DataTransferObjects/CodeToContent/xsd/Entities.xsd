  <xs:complexType name="UnknownRating">
    <xs:complexContent mixed="false">
      <xs:extension base="fev2:Rating">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="RatingDescriptors" type="sa:ArrayOfstring"/>
          <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="RatingName" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="OfflineCapabilities">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="Camera" type="xs:boolean"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="CoOpPlayers" type="fev2:Range"/>
      <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="DolbyDigital" type="xs:boolean"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="Players" type="fev2:Range"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="SystemLink" type="fev2:Range"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OnlineCapabilities">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="OnlineCoOpPlayers" type="fev2:Range"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="OnlineContentDownload" type="xs:boolean"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="OnlineLeaderBoards" type="xs:boolean"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="OnlineMultiPlayer" type="fev2:Range"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="OnlineVoice" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ProductImages">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="BoxArtFileUrl" type="fev2:URI"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="IconFileUrl" type="fev2:URI"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="ScreenshotFileUrls" type="sa:ArrayOfstring" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ProductVideos">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" nillable="false" name="ProductVideo" type="fev2:ProductVideo"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ProductVideo">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="VideoDefinition" type="xs:string"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="VideoFileUrls" type="sa:ArrayOfstring"/>
      <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="Extension" type="fev2:ProductVideoExtension"/>
      <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax"/>
    </xs:sequence>
    <xs:anyAttribute namespace="##any" processContents="lax"/>
  </xs:complexType>

  <xs:complexType name="Range">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="Max" type="xs:integer"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="Min" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CatalogResultExtension">
    <xs:sequence>
      <xs:any minOccurs="1" maxOccurs="unbounded" namespace="##targetNamespace" processContents="lax"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="GameCatalogMetaExtension">
    <xs:sequence>
      <xs:any minOccurs="1" maxOccurs="unbounded" namespace="##targetNamespace" processContents="lax"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PriceExtension">
    <xs:sequence>
      <xs:any minOccurs="1" maxOccurs="unbounded" namespace="##targetNamespace" processContents="lax"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ProductVideoExtension">
    <xs:sequence>
      <xs:any minOccurs="1" maxOccurs="unbounded" namespace="##targetNamespace" processContents="lax"/>
    </xs:sequence>
