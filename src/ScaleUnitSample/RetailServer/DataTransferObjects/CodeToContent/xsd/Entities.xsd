  <!--************************************-->
  <!--            TokenInfo           -->
  <!--************************************-->

  <xs:element name="TokenInfo" type="fev2:TokenInfo"/>

  <xs:complexType name="TokenInfo">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="LastUpdateOfStatus" type="xs:dateTime"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="PromotionName" type="xs:string"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="RedemptionDateTime" type="xs:dateTime"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="true"  name="TokenClassName" type="xs:string"/>
      <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="TokenStatus" type="xs:string">
        <xs:annotation><xs:documentation>
          One of possible statuses as specified by 'TokenStatus' enum.
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="TokenStatus">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Created">
        <xs:annotation>
          <xs:documentation>
            Usually not used in CIS ESD scenarios. Most Xbox Live tokens are
            loaded into CIS as "Activated"
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Activated">
        <xs:annotation>
          <xs:documentation>
            This is the state that tokens are in when returned from GetToken
            calls.  It means that they are ready to be sold.
            (Tokens can only be redeemed on a XBOX console if they are in the
            Activated state.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Redeemed">
        <xs:annotation>
          <xs:documentation>
            The token has been redeemed by the end customer.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Blacklisted">
        <xs:annotation>
          <xs:documentation>
            The token has been marked as "Do Not Use" in a CIS backend system.
            All activity external to CIS is not allowed and the state cannot be
            changed externally.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Invalid">
        <xs:annotation>
          <xs:documentation>
            CIS has no knowledge of the token. Either the token was never loaded
            into CIS or some part of the 5x5 was transposed or mistyped.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unknown">
        <xs:annotation>
          <xs:documentation>
            There was an internal issue retrieving the status of the token.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

</xs:schema>
